{
  "name": "tailwind-mobile",
  "description": "Mobile UI components made with Tailwind CSS",
  "version": "0.1.2",
  "private": true,
  "license": "MIT",
  "author": "Vladimir Kharlampidi",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^2.0.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build",
    "/*============ Kitchen Sink ============*/": "============",
    "react:webpack": "cross-env NODE_ENV=development webpack serve --config ./scripts/webpack-react.config.js",
    "react": "npm run react:webpack",
    "/*============ Dev tools ============*/": "============",
    "prettier": "prettier \"**/*.+(js|json|scss|css|less|ts|vue)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ext .js,.vue,.jsx,.svelte .",
    "validate": "npm-run-all --parallel check-format lint",
    "release": "npm run validate && node ./scripts/release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^10.2.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "exec-sh": "^0.3.4",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.1.0",
    "html-webpack-plugin": "^4.5.1",
    "inquirer": "^7.3.3",
    "mini-css-extract-plugin": "^1.3.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}