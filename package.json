{
  "name": "tailwind-mobile",
  "description": "Mobile UI components made with Tailwind CSS",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "author": "Vladimir Kharlampidi",
  "dependencies": {},
  "scripts": {
    "build": "cross-env NODE_ENV=production node scripts/build",
    "/*============ Kitchen Sink ============*/": "============",
    "react": "cross-env NODE_ENV=development vite ./kitchen-sink/react",
    "/*============ Dev tools ============*/": "============",
    "prettier": "prettier \"**/*.+(js|json|scss|css|less|ts|vue)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ext .js,.vue,.jsx,.svelte .",
    "validate": "npm-run-all --parallel check-format lint",
    "release": "npm run validate && node ./scripts/release"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "exec-sh": "^0.3.4",
    "framework7-icons": "^4.0.2",
    "fs-extra": "^9.1.0",
    "inquirer": "^7.3.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "tailwindcss": "^2.0.3",
    "vite": "^2.0.1"
  }
}
